  <Command>
    <Identifier>EjectTip</Identifier>
    <DisplayName>Eject Tip</DisplayName>
    <Description>Defintion of location and type of tip as well as headposition to realise individual ejects.</Description>
    <Observable>No</Observable>
    <Parameter>
      <Identifier>LabwareID</Identifier>
      <DisplayName>Labware ID</DisplayName>
      <Description>Unique name of labware item.</Description>
      <DataType>
        <Basic>String</Basic>
      </DataType>
    </Parameter>
    <Parameter>
      <Identifier>TipPosition</Identifier>
      <DisplayName>Tip Position</DisplayName>
      <Description>Select tips to be picked up. Either singularly or by range (“A1” or “A1:B2”). Consider LabwareId description!</Description>
      <DataType>
        <Basic>String</Basic>
      </DataType>
    </Parameter>
    <Parameter>
      <Identifier>HeadPosition</Identifier>
