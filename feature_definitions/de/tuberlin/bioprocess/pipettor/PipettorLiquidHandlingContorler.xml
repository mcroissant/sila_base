<?xml version="1.0" encoding="utf-8" ?>
<Feature SiLAVersion="2" MajorVersion="1" MinorVersion="0" Originator="de.tuberlin.bioprocess" UseCase="dispensing"
         xmlns="http://www.sila-standard.org"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.sila-standard.org https://gitlab.com/SiLA2/sila_features/raw/master/schema/FeatureDefinition.xsd">
  <Identifier>PipettorDeviceService</Identifier>
  <DisplayName>Pipettor Device Service</DisplayName>
  <Description>Basic Pipettor Commands. Adpated form the SiLA Pipettor Specifications (Version 1.0.2 | 2015-12-09)</Description>
  <Command>
    <Identifier>Aspirate</Identifier>
    <DisplayName>Aspirate</DisplayName>
    <Description>Aspirate a specified volume from well(s) in labware on workspace.</Description>
    <Observable>No</Observable>
    <Parameter>
      <Identifier>Volume</Identifier>
      <DisplayName>Volume</DisplayName>
      <Description>Volume in μL (>= 0)</Description>
      <DataType>
        <Basic>Quantity</Basic>
      </DataType>
    </Parameter>
    <Parameter>
      <Identifier>LabwareID</Identifier>
      <DisplayName>Labware ID</DisplayName>
      <Description>Unique name of labware item.</Description>
      <DataType>
        <Basic>String</Basic>
      </DataType>
    </Parameter>
    <Parameter>
      <Identifier>WellID</Identifier>
      <DisplayName>Well ID</DisplayName>
      <Description>Identifies the tip slots of the labware that contain tips of the tip component. If WellId is empty string TipRack is empty.</Description>
      <DataType>
        <Basic>Integer</Basic>
      </DataType>
    </Parameter>
    <Parameter>
      <Identifier>LiquidClass</Identifier>
      <DisplayName>Liquid Class</DisplayName>
      <Description>Liquidclass of liquid in source definition</Description>
      <DataType>
        <Basic>String</Basic>
      </DataType>
    </Parameter>
    <Parameter>
      <Identifier>HeadPosition</Identifier>
      <DisplayName>Head Position</DisplayName>
      <Description>Head position defines the liquid channels to be used for asprirating (“A1” or “B4:C6”). Definition is via matrix, therefore “B5:C6” contains liquid channels B5, B6, C5 and C6</Description>
      <DataType>
        <Basic>String</Basic>
      </DataType>
    </Parameter>
    <Parameter>
      <Identifier>AspiratePosition</Identifier>
      <DisplayName>Aspirate Position</DisplayName>
      <Description>Reference position for aspirate step. Values can be "LiquidLevel", "Bottom", "Top"</Description>
      <DataType>
        <Basic>String</Basic>
      </DataType>
    </Parameter>
    <Parameter>
      <Identifier>OffsetX</Identifier>
      <DisplayName>Offset X</DisplayName>
      <Description>Unit of offset in x direction is mm, default is 0.0.</Description>
      <DataType>
        <Basic>Real</Basic>
      </DataType>
    </Parameter>
    <Parameter>
      <Identifier>OffsetY</Identifier>
      <DisplayName>Offset Y</DisplayName>
      <Description>Unit of offset in y direction is mm, default is 0.0.</Description>
      <DataType>
        <Basic>Real</Basic>
      </DataType>
    </Parameter>
    <Response>
      <Identifier>Undertaken</Identifier>
      <DisplayName>Undertaken</DisplayName>
      <Description>TRUE if the Command has been untertaken, FALSE if not. </Description>
      <DataType>
        <Basic>Boolean</Basic>
      </DataType>
    </Response>
  </Command>
